{
  "name": "antigravity-to-openai",
  "version": "1.0.0",
  "description": "Antigravity API 转 OpenAI 格式的代理服务",
  "type": "module",
  "main": "src/server/index.ts",
  "scripts": {
    "start": "node --expose-gc --import tsx src/server/index.ts",
    "dev": "node --expose-gc --import tsx --watch src/server/index.ts",
    "login": "tsx scripts/oauth-server.ts",
    "type-check": "tsc --noEmit && pnpm --dir client exec tsc -b",
    "lint": "pnpm --dir client run lint",
    "check": "pnpm run type-check && pnpm run lint",
    "build": "pnpm --dir client install && pnpm --dir client run build"
  },
  "keywords": [
    "antigravity",
    "openai",
    "api",
    "proxy"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "adm-zip": "~0.5.16",
    "archiver": "~7.0.1",
    "express": "~5.2.1",
    "gpt-tokenizer": "~3.4.0",
    "helmet": "~8.1.0",
    "multer": "~2.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/adm-zip": "~0.5.7",
    "@types/archiver": "~7.0.0",
    "@types/express": "~5.0.6",
    "@types/multer": "~2.0.0",
    "@types/node": "~24.10.1",
    "tsx": "~4.21.0",
    "typescript": "~5.9.3"
  }
}